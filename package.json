{
  "name": "clean-slate-mikro",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/konstantino-vichi/clean-slate-mikro.git",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "pre-build": "echo 'install dependencies'",
    "build": "tsc",
    "start": "yarn build && npx mikro-orm schema:update --run --fk-checks",
    "test": "echo 'test'",
    "migrate-all": "npx mikro-orm migration:create -d"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/mikro-orm.config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.4.2",
    "@mikro-orm/core": "^5.4.2",
    "@mikro-orm/migrations": "^5.4.2",
    "@mikro-orm/postgresql": "^5.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.5"
  }
}
